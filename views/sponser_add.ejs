<!DOCTYPE html>
<html lang="en">

<head>
    <meta charset="UTF-8">
    <title>Add staff</title>
    <link href="/css/styles.css" rel="stylesheet" type="text/css">

</head>

<body>
    <a class="back" href="/sponser">Back</a>
    <h1 class="info_h1">Add a new sponser</h1>
    <table class="sponser_table">

        <form class="form-horizontal" action="/sponser/add" method="post">
            <!-- Text -->
            <tr>
                <td>
                    <span>SponserID</span>
                </td>
                <td>
                    <input name="sponserID" type="text" class="leipiplugins" leipiplugins="text">
                </td>
            </tr>

            </div>

            <!-- Text -->
            <tr>
                <td>
                    <span>Name</span>
                </td>
                <td>
                    <input name="name" type="text" class="leipiplugins" leipiplugins="text">
                </td>
            </tr>

            <!-- Text -->
            <tr>
                <td>
                    <span>StartTime</span>
                </td>
                <td>
                    <input name="startTime" type="text" class="leipiplugins" leipiplugins="text">
                </td>
            </tr>

            <!-- Text -->
            <tr>
                <td>
                    <span>TeamName</span>
                </td>
                <td>
                    <input name="teamName" type="text" placeholder="Celtics" class="leipiplugins" leipiplugins="text">
                </td>
            </tr>
            <tr>
                <td>
                    <input class="btn" type="submit" value="submit">
                </td>
            </tr>            
        </form>

    </table>

</body>

</html>